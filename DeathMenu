//Code for GameOver menu when you collide with an Object

using UnityEngine;

public class DeathMenu : MonoBehaviour
{




    // Start is called before the first frame update
    void Start()
    {
        gameObject.SetActive(false); //Turns off GameOver
    }

    public void ToggleEndMenu(float score)
    {
        gameObject.SetActive(true); //Turns on Gameover
    }

}
